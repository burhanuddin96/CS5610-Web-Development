<h2>Listing Managers</h2>

<table class="table">
  <thead>
    <tr>
      <th>Email</th>
      <th>Name</th>
      <th>ID</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for user <- @users do %>
    <tr>
      <td><%= user.email %></td>
      <td><%= user.name %></td>
      <td><%= user.id %></td>
    </tr>
<% end %>
  </tbody>
</table>

<span><%= link "Back", to: user_path(@conn, :new) %></span>
